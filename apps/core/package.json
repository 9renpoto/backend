{
  "name": "@9renpoto/core",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:prod": "node dist/main.js",
    "test": "jest",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.5.0",
    "@nestjs/common": "^7.5.3",
    "@nestjs/config": "0.6.3",
    "@nestjs/core": "^7.5.3",
    "@nestjs/graphql": "^7.8.2",
    "@nestjs/platform-fastify": "^7.5.3",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/terminus": "^7.0.0",
    "@nestjs/typeorm": "^7.1.5",
    "@types/graphql": "^14.5.0",
    "apollo-server-fastify": "^2.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "fastify-swagger": "^4.0.0",
    "graphql": "^15.0.0",
    "graphql-relay": "^0.6.0",
    "graphql-tag-pluck": "^0.8.7",
    "nest-raven": "^7.0.0",
    "nestjs-dataloader": "^7.0.1",
    "nestjs-graphql-relay": "^1.0.2",
    "nestjs-stripe": "^0.4.0",
    "pg": "^8.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "stripe": "^8.19.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.30",
    "typeorm-naming-strategies": "^2.0.0",
    "typeorm-test-transactions": "^1.9.0",
    "typeorm-transactional-cls-hooked": "^0.1.20"
  },
  "devDependencies": {
    "@nestjs/cli": "7.6.0",
    "@nestjs/testing": "7.6.15",
    "apollo-server-testing": "2.24.0",
    "typeorm-factory": "0.0.14"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "collectCoverage": true,
    "testEnvironment": "node"
  }
}
