datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:password@127.0.0.1:15432/nexus_prisma"
}

generator prisma_client {
  provider = "prisma-client-js"
}

model User {
  id        String   @default(cuid()) @id
  email     String   @unique
  birthDate DateTime
  metadata  Json
  posts     Post[]
}

model Post {
  id      String @default(cuid()) @id
  authors User[] @relation(references: [id])
}
